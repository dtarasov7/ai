{
  "uid": "pgpro-otel-extended-overview-raw",
  "title": "PostgreSQL (pgpro-otel-collector) – Overview + Raw",
  "schemaVersion": 38,
  "version": 1,
  "editable": true,
  "style": "dark",
  "timezone": "browser",
  "refresh": "30s",
  "time": { "from": "now-6h", "to": "now" },
  "tags": ["postgres", "pgpro-otel-collector", "prometheus", "overview", "raw", "rate_window"],
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "label": "Datasource",
        "hide": 2,
        "query": "prometheus",
        "current": { "text": "Prometheus", "value": "Prometheus", "selected": false }
      },
      {
        "name": "rate_window",
        "type": "custom",
        "label": "Rate window",
        "hide": 0,
        "multi": false,
        "includeAll": false,
        "query": "1m,2m,5m,10m,30m",
        "current": { "text": "2m", "value": "2m", "selected": true },
        "options": [
          { "text": "1m", "value": "1m", "selected": false },
          { "text": "2m", "value": "2m", "selected": true },
          { "text": "5m", "value": "5m", "selected": false },
          { "text": "10m", "value": "10m", "selected": false },
          { "text": "30m", "value": "30m", "selected": false }
        ]
      },
      {
        "name": "instance",
        "type": "query",
        "label": "Postgresql instance",
        "hide": 0,
        "multi": true,
        "includeAll": true,
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": { "query": "label_values(postgresql_up, instance)", "refId": "Q1" },
        "definition": "label_values(postgresql_up, instance)",
        "current": { "text": ".*", "value": ".*", "selected": false },
        "refresh": 2,
        "sort": 1
      },
      {
        "name": "database",
        "type": "query",
        "label": "Database",
        "hide": 0,
        "multi": true,
        "includeAll": true,
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": { "query": "label_values(postgresql_databases_size_bytes{instance=~\"$instance\"}, database)", "refId": "Q2" },
        "definition": "label_values(postgresql_databases_size_bytes{instance=~\"$instance\"}, database)",
        "current": { "text": ".*", "value": ".*", "selected": false },
        "refresh": 2,
        "sort": 1
      },
      {
        "name": "table",
        "type": "query",
        "label": "Table",
        "hide": 0,
        "multi": true,
        "includeAll": true,
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": { "query": "label_values(postgresql_tables_size_bytes{instance=~\"$instance\", database=~\"$database\"}, table)", "refId": "Q3" },
        "definition": "label_values(postgresql_tables_size_bytes{instance=~\"$instance\", database=~\"$database\"}, table)",
        "current": { "text": ".*", "value": ".*", "selected": false },
        "refresh": 2,
        "sort": 1
      }
    ]
  },
  "panels": [
    {
      "type": "stat",
      "title": "Up",
      "id": 1,
      "gridPos": { "x": 0, "y": 0, "w": 3, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [{ "refId": "A", "expr": "max(postgresql_up{instance=~\"$instance\"})" }],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "values": false, "fields": "" },
        "orientation": "horizontal",
        "textMode": "value",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },

    {
      "type": "stat",
      "title": "Server version",
      "id": 2,
      "gridPos": { "x": 3, "y": 0, "w": 3, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "refId": "A",
          "expr": "max(postgresql_server_version{instance=~\"$instance\"})"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "values": false, "fields": "" },
        "orientation": "horizontal",
        "textMode": "value",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },

    {
      "type": "stat",
      "title": "Vacuum",
      "id": 3,
      "gridPos": { "x": 6, "y": 0, "w": 3, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "refId": "A",
          "expr": "max(postgresql_vacuum_running{instance=~\"$instance\"})"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "values": false, "fields": "" },
        "orientation": "horizontal",
        "textMode": "value",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },

    {
      "type": "stat",
      "title": "Vacuum time",
      "id": 4,
      "gridPos": { "x": 9, "y": 0, "w": 3, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "targets": [
        {
          "refId": "A",
          "expr": "max(postgresql_vacuum_duration_seconds{instance=~\"$instance\"})"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "values": false, "fields": "" },
        "orientation": "horizontal",
        "textMode": "value",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },

    {
      "type": "stat",
      "title": "Failed sessions",
      "id": 5,
      "gridPos": { "x": 12, "y": 0, "w": 3, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(increase(postgresql_sessions_failed_total{instance=~\"$instance\"}[$rate_window]))"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "values": false, "fields": "" },
        "orientation": "horizontal",
        "textMode": "value",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },

    {
      "type": "stat",
      "title": "Rollbacks",
      "id": 6,
      "gridPos": { "x": 15, "y": 0, "w": 3, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(increase(postgresql_databases_rollbacks_total{instance=~\"$instance\", database=~\"$database\"}[$rate_window]))"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "values": false, "fields": "" },
        "orientation": "horizontal",
        "textMode": "value",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },

    {
      "type": "stat",
      "title": "Deadlocks",
      "id": 7,
      "gridPos": { "x": 18, "y": 0, "w": 3, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(increase(postgresql_databases_deadlocks_total{instance=~\"$instance\", database=~\"$database\"}[$rate_window]))"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "values": false, "fields": "" },
        "orientation": "horizontal",
        "textMode": "value",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },

    {
      "type": "stat",
      "title": "Waiting time",
      "id": 8,
      "gridPos": { "x": 21, "y": 0, "w": 3, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "unit": "ms" }, "overrides": [] },
      "targets": [
        {
          "refId": "A",
          "expr": "max(postgresql_locks_waiting_max_duration_milliseconds{instance=~\"$instance\", database=~\"$database\"})"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "values": false, "fields": "" },
        "orientation": "horizontal",
        "textMode": "value",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },

    {
      "type": "timeseries",
      "title": "Transactions rate",
      "id": 9,
      "gridPos": { "x": 0, "y": 4, "w": 12, "h": 6 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["max"], "sortBy": "Max", "sortDesc": true }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (database) (rate(postgresql_databases_commits_total{instance=~\"$instance\", database=~\"$database\"}[$rate_window]))",
          "legendFormat": "commits {{database}}"
        },
        {
          "refId": "B",
          "expr": "sum by (database) (rate(postgresql_databases_rollbacks_total{instance=~\"$instance\", database=~\"$database\"}[$rate_window]))",
          "legendFormat": "rollbacks {{database}}"
        }
      ]
    },

    {
      "type": "stat",
      "title": "Connections",
      "id": 10,
      "gridPos": { "x": 12, "y": 4, "w": 3, "h": 3 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(postgresql_activity_connections{instance=~\"$instance\", database=~\"$database\"})"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "values": false, "fields": "" }, "orientation": "horizontal", "textMode": "value", "colorMode": "value", "graphMode": "none" }
    },

    {
      "type": "stat",
      "title": "Active",
      "id": 11,
      "gridPos": { "x": 15, "y": 4, "w": 3, "h": 3 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "refId": "A", "expr": "sum(postgresql_activity_connections{instance=~\"$instance\", database=~\"$database\", state=\"active\"})" }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "values": false, "fields": "" }, "orientation": "horizontal", "textMode": "value", "colorMode": "value", "graphMode": "none" }
    },

    {
      "type": "stat",
      "title": "Idle transactions",
      "id": 12,
      "gridPos": { "x": 18, "y": 4, "w": 3, "h": 3 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "refId": "A", "expr": "sum(postgresql_activity_connections{instance=~\"$instance\", database=~\"$database\", state=~\"idle in transaction.*\"})" }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "values": false, "fields": "" }, "orientation": "horizontal", "textMode": "value", "colorMode": "value", "graphMode": "none" }
    },

    {
      "type": "stat",
      "title": "Waiting",
      "id": 13,
      "gridPos": { "x": 21, "y": 4, "w": 3, "h": 3 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "refId": "A", "expr": "sum(postgresql_locks_waiting{instance=~\"$instance\", database=~\"$database\"})" }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "values": false, "fields": "" }, "orientation": "horizontal", "textMode": "value", "colorMode": "value", "graphMode": "none" }
    },

    {
      "type": "piechart",
      "title": "Wait events",
      "id": 14,
      "gridPos": { "x": 12, "y": 7, "w": 12, "h": 6 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["max"], "sortBy": "Max", "sortDesc": true },
        "pieType": "donut",
        "reduceOptions": { "calcs": ["lastNotNull"], "values": false }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(8, sum by (wait_event_type, wait_event) (postgresql_activity_wait_events{instance=~\"$instance\", database=~\"$database\"}))",
          "legendFormat": "{{wait_event_type}}.{{wait_event}}"
        }
      ]
    },

    {
      "type": "timeseries",
      "title": "Connections over time",
      "id": 15,
      "gridPos": { "x": 0, "y": 10, "w": 12, "h": 6 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["max"], "sortBy": "Max", "sortDesc": true }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (state) (postgresql_activity_connections{instance=~\"$instance\", database=~\"$database\"})",
          "legendFormat": "{{state}}"
        }
      ]
    },

    {
      "type": "timeseries",
      "title": "Locks over time, by mode",
      "id": 16,
      "gridPos": { "x": 12, "y": 13, "w": 12, "h": 6 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["max"], "sortBy": "Max", "sortDesc": true }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (mode) (postgresql_locks_by_mode{instance=~\"$instance\", database=~\"$database\"})",
          "legendFormat": "{{mode}}"
        }
      ]
    },

    {
      "type": "timeseries",
      "title": "Wait events, top-5",
      "id": 17,
      "gridPos": { "x": 12, "y": 19, "w": 12, "h": 6 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["max"], "sortBy": "Max", "sortDesc": true }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(5, sum by (wait_event_type, wait_event) (postgresql_activity_wait_events{instance=~\"$instance\", database=~\"$database\"}))",
          "legendFormat": "{{wait_event_type}}.{{wait_event}}"
        }
      ]
    },

    {
      "type": "row",
      "title": "Raw",
      "id": 100,
      "gridPos": { "x": 0, "y": 25, "w": 24, "h": 1 },
      "collapsed": false,
      "panels": []
    },

    {
      "type": "timeseries",
      "title": "Temp files / bytes (per second)",
      "id": 101,
      "gridPos": { "x": 0, "y": 26, "w": 12, "h": 6 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "options": { "legend": { "displayMode": "table", "placement": "right", "calcs": ["max"], "sortBy": "Max", "sortDesc": true } },
      "fieldConfig": { "defaults": { "unit": "bytes" }, "overrides": [] },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (database) (rate(postgresql_databases_temp_bytes_total{instance=~\"$instance\", database=~\"$database\"}[$rate_window]))",
          "legendFormat": "temp bytes/sec {{database}}"
        },
        {
          "refId": "B",
          "expr": "sum by (database) (rate(postgresql_databases_temp_files_total{instance=~\"$instance\", database=~\"$database\"}[$rate_window]))",
          "legendFormat": "temp files/sec {{database}}"
        }
      ]
    },

    {
      "type": "timeseries",
      "title": "Cache hit ratio",
      "id": 102,
      "gridPos": { "x": 12, "y": 26, "w": 12, "h": 6 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "options": { "legend": { "displayMode": "table", "placement": "right", "calcs": ["max"], "sortBy": "Max", "sortDesc": true } },
      "fieldConfig": { "defaults": { "unit": "percentunit", "min": 0, "max": 1 }, "overrides": [] },
      "targets": [
        {
          "refId": "A",
          "expr": "avg by (database) (postgresql_cache_hit_ratio{instance=~\"$instance\", database=~\"$database\"})",
          "legendFormat": "{{database}}"
        }
      ]
    },

    {
      "type": "timeseries",
      "title": "WAL: bytes / records / FPI (per second)",
      "id": 103,
      "gridPos": { "x": 0, "y": 32, "w": 12, "h": 6 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "options": { "legend": { "displayMode": "table", "placement": "right", "calcs": ["max"], "sortBy": "Max", "sortDesc": true } },
      "fieldConfig": { "defaults": { "unit": "bytes" }, "overrides": [] },
      "targets": [
        { "refId": "A", "expr": "rate(postgresql_wal_bytes_total{instance=~\"$instance\"}[$rate_window])", "legendFormat": "WAL bytes/sec" },
        { "refId": "B", "expr": "rate(postgresql_wal_records_total{instance=~\"$instance\"}[$rate_window])", "legendFormat": "WAL records/sec" },
        { "refId": "C", "expr": "rate(postgresql_wal_fpi_total{instance=~\"$instance\"}[$rate_window])", "legendFormat": "WAL FPI/sec" }
      ]
    },

    {
      "type": "timeseries",
      "title": "WAL write/sync time (seconds/sec)",
      "id": 104,
      "gridPos": { "x": 12, "y": 32, "w": 12, "h": 6 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "options": { "legend": { "displayMode": "table", "placement": "right", "calcs": ["max"], "sortBy": "Max", "sortDesc": true } },
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "targets": [
        { "refId": "A", "expr": "rate(postgresql_wal_write_time_milliseconds_total{instance=~\"$instance\"}[$rate_window]) / 1000", "legendFormat": "wal write time (s/sec)" },
        { "refId": "B", "expr": "rate(postgresql_wal_sync_time_milliseconds_total{instance=~\"$instance\"}[$rate_window]) / 1000", "legendFormat": "wal sync time (s/sec)" }
      ]
    },

    {
      "type": "timeseries",
      "title": "Bgwriter buffers (per second)",
      "id": 105,
      "gridPos": { "x": 0, "y": 38, "w": 24, "h": 6 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "options": { "legend": { "displayMode": "table", "placement": "right", "calcs": ["max"], "sortBy": "Max", "sortDesc": true } },
      "fieldConfig": { "defaults": { "unit": "ops" }, "overrides": [] },
      "targets": [
        { "refId": "A", "expr": "rate(postgresql_bgwriter_buffers_checkpoint_total{instance=~\"$instance\"}[$rate_window])", "legendFormat": "buffers checkpoint/sec" },
        { "refId": "B", "expr": "rate(postgresql_bgwriter_buffers_clean_total{instance=~\"$instance\"}[$rate_window])", "legendFormat": "buffers clean/sec" },
        { "refId": "C", "expr": "rate(postgresql_bgwriter_buffers_backend_total{instance=~\"$instance\"}[$rate_window])", "legendFormat": "buffers backend/sec" },
        { "refId": "D", "expr": "rate(postgresql_bgwriter_buffers_alloc_total{instance=~\"$instance\"}[$rate_window])", "legendFormat": "buffers alloc/sec" }
      ]
    },

    {
      "type": "timeseries",
      "title": "Buffer cache (pg_buffercache): used / dirty / pinned / unused",
      "id": 106,
      "gridPos": { "x": 0, "y": 44, "w": 24, "h": 7 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "options": { "legend": { "displayMode": "table", "placement": "right", "calcs": ["max"], "sortBy": "Max", "sortDesc": true } },
      "targets": [
        { "refId": "A", "expr": "postgresql_buffercache_used{instance=~\"$instance\"}", "legendFormat": "used" },
        { "refId": "B", "expr": "postgresql_buffercache_dirty{instance=~\"$instance\"}", "legendFormat": "dirty" },
        { "refId": "C", "expr": "postgresql_buffercache_pinned{instance=~\"$instance\"}", "legendFormat": "pinned" },
        { "refId": "D", "expr": "postgresql_buffercache_unused{instance=~\"$instance\"}", "legendFormat": "unused" }
      ]
    },

    {
      "type": "timeseries",
      "title": "Top 10 tables by seq scans (per second)",
      "id": 107,
      "gridPos": { "x": 0, "y": 51, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "options": { "legend": { "displayMode": "table", "placement": "right", "calcs": ["max"], "sortBy": "Max", "sortDesc": true } },
      "fieldConfig": { "defaults": { "unit": "ops" }, "overrides": [] },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(10, sum by (database, table) (rate(postgresql_tables_seq_scan_total{instance=~\"$instance\", database=~\"$database\", table=~\"$table\"}[$rate_window])))",
          "legendFormat": "{{database}}/{{table}}"
        }
      ]
    },

    {
      "type": "timeseries",
      "title": "Top 10 tables by index scans (per second)",
      "id": 108,
      "gridPos": { "x": 12, "y": 51, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "options": { "legend": { "displayMode": "table", "placement": "right", "calcs": ["max"], "sortBy": "Max", "sortDesc": true } },
      "fieldConfig": { "defaults": { "unit": "ops" }, "overrides": [] },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(10, sum by (database, table) (rate(postgresql_tables_index_scan_total{instance=~\"$instance\", database=~\"$database\", table=~\"$table\"}[$rate_window])))",
          "legendFormat": "{{database}}/{{table}}"
        }
      ]
    }
  ]
}

